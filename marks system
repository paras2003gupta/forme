// MARKS SYSTEM
#include <iostream>
using namespace std;

int main() {
    int m;
    cout<<"ENter the marks of students"<<endl;
    cin>>m;
    if(m>90) 
    cout<<"YOUR GRADE IS A+ "<<endl;
     else if (m>80) 
    cout<<"YOUR GRADE IS A"<<endl;
     else if (m>70) 
    cout<<"YOUR GRADE IS B+"<<endl;
     else if (m>60) 
    cout<<"YOUR GRADE IS B"<<endl;
     else if (m>50) 
    cout<<"YOUR GRADE IS C"<<endl;
     else if (m>40) 
    cout<<"YOUR GRADE IS D"<<endl;
     else if (m>30) 
    cout<<"YOUR GRADE IS E"<<endl;
     else
     cout<<"YOUR GRADE IS F"<<endl;
     
    
   
   
          
   
   
   
   
}
